{
  "version": "1.0.0-*",

  "buildOptions": {
    "additionalArguments": [ "/additionalfile:../stylecop.json" ],
    "compile": "../GlobalSuppressions.cs",
    "warningsAsErrors": true,
    "xmlDoc": true
  },

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },

    "Release": {
      "buildOptions": {
        "debugType": "none",
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Crest.Core": {
      "target": "project"
    },
    "DryIoc.Internal": {
      "type": "build",
      "version": "2.9.0"
    },
    "Microsoft.Extensions.DependencyModel": "1.0.0",
    "NETStandard.Library": "1.6.0",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "System.ComponentModel.TypeConverter": "4.1.0"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },

  "scripts": {
    "precompile": "powershell ../../build/exclude_dry_ioc_from_stylecop.ps1"
  }
}
